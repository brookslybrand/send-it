{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "db:init": "prisma db push && prisma db seed",
    "styles:build": "tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/index.css",
    "styles:watch": "npm run styles:build -- --watch",
    "prebuild": "npm run styles:build && prisma generate",
    "build": "remix build",
    "remix:dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "predev": "prisma generate",
    "dev": "npm-run-all --parallel styles:watch remix:dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx ./app",
    "pretty-quick": "pretty-quick --staged"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@reach/visually-hidden": "^0.16.0",
    "@remix-run/react": "^1.3.5",
    "@remix-run/serve": "^1.3.5",
    "@remix-run/vercel": "^1.3.5",
    "@supabase/supabase-js": "^1.33.3",
    "clsx": "^1.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^v6.3.0",
    "remix": "^1.3.5",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "remix-auth-supabase": "^3.1.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.5",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
