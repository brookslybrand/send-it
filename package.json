{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "styles:build": "tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/index.css",
    "styles:watch": "npm run styles:build -- --watch",
    "build": "npm run styles:build && remix build",
    "remix:dev": "remix dev",
    "dev": "npm-run-all --parallel styles:watch remix:dev",
    "postinstall": "remix setup node",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "remix-serve build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.4.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "husky": "^6.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
