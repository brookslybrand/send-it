{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "db:init": "prisma db push && prisma db seed",
    "styles:build": "tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/index.css",
    "styles:watch": "npm run styles:build -- --watch",
    "prebuild": "npm run styles:build && prisma generate",
    "build": "remix build",
    "remix:dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "predev": "prisma generate",
    "dev": "npm-run-all --parallel styles:watch remix:dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx ./app",
    "pretty-quick": "pretty-quick --staged"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@reach/visually-hidden": "^0.16.0",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "@supabase/supabase-js": "^1.29.4",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^v6.2.1",
    "remix": "^1.1.3",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "remix-auth-supabase": "^3.1.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.5",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
